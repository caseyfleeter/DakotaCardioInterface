	print("Model and branches")
	# Now, get TABP and TAWSS quantities across entire model
	modelLowTAWSS = np.zeros(len(thresholds))

	modelMaxTABP = -sys.float_info.max
	modelMinTABP = sys.float_info.max
	modelMeanTABP = 0.0

	branchesMaxTABP = [-sys.float_info.max for i in range(0,NUM_BRANCHES)]
	branchesMinTABP = [sys.float_info.max for i in range(0,NUM_BRANCHES)]
	branchesMeanTABP = [0.0 for i in range(0,NUM_BRANCHES)]

	stripsMaxTABP = [-sys.float_info.max for i in range(0,NUM_STRIPS)]
	stripsMinTABP = [sys.float_info.max for i in range(0,NUM_STRIPS)]
	stripsMeanTABP = [0.0 for i in range(0,NUM_STRIPS)]

	modelMaxTAWSS = -sys.float_info.max
	modelMinTAWSS = sys.float_info.max
	modelMeanTAWSS = 0.0

	branchesMaxTAWSS = [-sys.float_info.max for i in range(0,NUM_BRANCHES)]
	branchesMinTAWSS = [sys.float_info.max for i in range(0,NUM_BRANCHES)]
	branchesMeanTAWSS = [0.0 for i in range(0,NUM_BRANCHES)]

