	branchesMeanTAWSS = [wss / a for wss,a in zip(branchesMeanTAWSS, branchesTotalArea)]

	# if(threshold_check):
	# make lowWSS a percentage of total area
	modelLowTAWSS = [l / modelTotalArea for l in modelLowTAWSS]


