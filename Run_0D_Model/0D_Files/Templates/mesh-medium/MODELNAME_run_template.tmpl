	pCT  = [output_QI[52][START_TIME:END_TIME],output_QI[53][START_TIME:END_TIME]] 
	pLII = [output_QI[62][START_TIME:END_TIME]] 
	pLR  = [output_QI[56][START_TIME:END_TIME]] 
	pRR  = [output_QI[55][START_TIME:END_TIME]] 
	pRI  = [output_QI[57][START_TIME:END_TIME],output_QI[58][START_TIME:END_TIME]] 
	pRII = [output_QI[59][START_TIME:END_TIME]] 
	pS   = [output_QI[54][START_TIME:END_TIME]] 
	pLI  = [output_QI[60][START_TIME:END_TIME],output_QI[61][START_TIME:END_TIME]] 
