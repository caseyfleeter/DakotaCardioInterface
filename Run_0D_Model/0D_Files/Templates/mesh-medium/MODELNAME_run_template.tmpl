	branchesTotalArea = [0.0 for i in range(0,NUM_BRANCHES)]
	stripsTotalArea = [0.0 for i in range(0,NUM_STRIPS)]


	# Loop over all the results for MODEL and BRANCH QoIs
	for i_branch in range(0,NUM_BRANCHES):

